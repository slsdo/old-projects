{
  "name": "tmdict",
  "version": "1.0.0",
  "devDependencies": {
    "@types/fs-extra": "^5.1.0",
    "@types/js-beautify": "^1.8.2",
    "@types/js-yaml": "^3.12.3",
    "@types/page": "^1.8.0",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "commander": "^2.20.3",
    "eslint": "^6.8.0",
    "fs-extra": "^7.0.1",
    "js-beautify": "^1.11.0",
    "js-yaml": "^3.13.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "rimraf": "^3.0.2",
    "sirv-cli": "^0.4.5",
    "sitemap": "^2.2.0",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^5.4.5",
    "typescript": "^3.8.3",
    "walkdir": "0.0.12",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "page": "^1.11.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "scripts": {
    "asset": "node bin/main.js --asset",
    "build": "run-s prebuild asset data html css webpack",
    "build:prod": "run-s prebuild asset data:prod html:prod css webpack:prod",
    "clean": "rimraf bin dist",
    "compile": "tsc",
    "css": "node-sass --output-style compressed ./src/css/bamboo.scss ./dist/src/css/style.min.css && node-sass --output-style compressed ./src/css/kara.scss ./dist/src/css/book.min.css",
    "data": "node bin/main.js -d",
    "data:prod": "node bin/main.js -d production",
    "help": "npm run compile && node bin/main.js --help",
    "html": "node bin/main.js -h && npm run sitemap",
    "html:prod": "node bin/main.js -h production && npm run sitemap",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "prebuild": "run-s clean prebuild:data compile",
    "prebuild:data": "cp -r ./data/data ./src/components/",
    "server": "webpack-dev-server --open",
    "sitemap": "node bin/main.js -s",
    "start": "sirv dist",
    "test": "echo \"Error: no test specified\" && exit 1",
    "webpack": "webpack --config webpack.dev.config.js",
    "webpack:prod": "webpack --config webpack.prod.config.js"
  }
}
